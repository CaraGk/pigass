{% extends 'layout.html.twig' %}

{% block title %}{{ parent() }} - Adhésion - Questions complémentaires{% endblock %}

{% block headline %}Questions complémentaires{% endblock %}

{% block action %}
    {% if ato == true %}
        <li class="btn btn-primary">
            <a href="{{ path('user_register_list', {'skip': true, 'userid': person.user.id}) }}" title="Passer cette étape">
